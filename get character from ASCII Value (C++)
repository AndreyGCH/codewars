Objective C

Solution

#import <Foundation/Foundation.h>
NSString* get_char(int i) {
  return  [NSString stringWithFormat:@"%c", i];
}

Test 

#import <Foundation/Foundation.h>
#include <stdlib.h>

@implementation TestSuite
    - (void) tests1
    {
        UKStringsEqual(@"A", get_char(65));
    }
@end


C++

Solution

char get_char(int i) {
  return char(i);
}
